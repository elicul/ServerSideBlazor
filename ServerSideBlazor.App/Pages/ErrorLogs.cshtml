@using ServerSideBlazor.App.Services
@page "/error-logs"
@inject ErrorLogService ErrorLogService

<h1>Counter</h1>

<p>Current count: @currentCount</p>
<p>Current count: @errorLog</p>

<button class="btn btn-primary" onclick="@IncrementCount">Click me</button>

@functions {
    int currentCount = 0;
    string errorLog = "";

    void IncrementCount()
    {
        currentCount++;
        errorLog = ErrorLogService.GetJsonFile();
    }
}
